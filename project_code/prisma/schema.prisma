generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

// model table
model User {
  id    String  @id @default(auto()) @map("_id") @db.ObjectId
  name String
  email String @unique
  password String
  identity UserRole
}

enum UserRole {
  student
  tutor
  siteAdmin
  tutorAdmin
}





